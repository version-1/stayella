<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>

<GridPane fx:id="container" fx:controller="group1.stayella.Controller.ControllerCharges"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.172-ea" hgap="50" vgap="15">

<Label fx:id="total" text="Total :"
           GridPane.columnIndex="1"
           GridPane.rowIndex="11"
            alignment="CENTER_RIGHT"/>
<Label fx:id="sumOfCharge" text="$ 0.0"
       GridPane.columnIndex="2"
       GridPane.rowIndex="11"/>
    <Button fx:id="submit" GridPane.columnIndex="2"
            GridPane.rowIndex="12" text="Submit" onAction="#onSubmitAction"/>
    <Button fx:id="close" GridPane.columnIndex="1"
            GridPane.rowIndex="13" text="close" onAction="#closeAction"/>
    <Button fx:id="reset" GridPane.columnIndex="2"
            GridPane.rowIndex="13" text="Reset" onAction="#onResetAction"/>


</GridPane>

